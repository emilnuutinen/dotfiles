format.inputdate=2
format.notifydate=%a %F
format.notifytime=%T
format.appointmenttime=%H:%M
format.outputdate=%d-%m-%y
format.dayheading=%a %d/%m/%y
general.autogc=no
general.autosave=yes
general.confirmdelete=yes
general.confirmquit=no
general.firstdayofweek=monday
general.multipledays=yes
general.periodicsave=0
general.systemevents=yes
notification.command=CONT="$(calcurse -n | tail -1 | cut -d ' ' -f 4-)" && notify-send -t 0 -i NONE "calcurse" "$CONT"
notification.notifyall=all
notification.warning=1800
